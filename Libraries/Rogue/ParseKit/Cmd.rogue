module ParseKit

class Cmd<<$ParserType>>( t:Token<<$ParserType>> )
endClass

class CmdUnary<<$ParserType>>( t, operand:Cmd ) : Cmd<<$ParserType>> [abstract]
  METHODS
    method op->String [abstract]

    method to->String
      return "($$)" (op,left)
endClass

class CmdBinary<<$ParserType>>( t, left:Cmd<<$ParserType>>, right:Cmd<<$ParserType>> ) : Cmd<<$ParserType>> [abstract]
  METHODS
    method op->String [abstract]

    method to->String
      return "($ $ $)" (left,op,right)
endClass

