module Plasmacore

class GGSpacer : GGComponent
  GLOBAL METHODS
    method horizontal( pixels:Int32 )->GGSpacer
      return GGSpacer( XY(pixels,1) )

    method vertical( pixels:Int32 )->GGSpacer
      return GGSpacer( XY(1,pixels) )

  METHODS
    method init( fixed_size )
      size = fixed_size

endClass

